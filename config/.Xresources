! Zsh green folder
URxvt*color2: #8AE234

! vim yellow
URxvt*color11: #C4A000
! yellow
URxvt*color3: #C4A000
!#FCE94F

! ls dirs, vim empty lines
URxvt*color12: #3465A4
! boh
URxvt*color4: #729FCF

! Strings
