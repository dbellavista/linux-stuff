! Using xft
URxvt*termName: rxvt-unicode-256color
URxvt*font:	xft:Inconsolata:size=11
URxvt.scrollBar:	false
URxvt.print-pipe: "cat > /dev/null"

Xft*dpi:	96
Xft*antialiasing:	True
Xtf*hinting:	Full

! Perl extensions
URxvt.perl-ext-common: default,clipboard,matcher,keyboard-select

!! URLs
URxvt.keysym.C-U: perl:url-select:select_next
URxvt.url-launcher: /usr/bin/google-chrome
URxvt.underlineURLs: True
URxvt.matcher.button: 1
!! Copy-Paste
URxvt.clipboard.autocopy: true
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.copyCommand: xsel -ib
URxvt.pasteCommand: xsel -ob

! Transparent
URxvt*.transparent: true
URxvt*.shading: 5

! Colors

URxvt*color0: #000000
! Text when selecting
URxvt*color8: #555753

! Boh
URxvt*color9: #0000FF
!#CC0000
! Boh
URxvt*color1: #FF0000
!#EF2929

! ls Executable files
